{
  "Description": "A stack that creates any resources required for template.yml to depend on.",
  "Resources": {
    "Bucket": {
      "Type": "AWS::S3::Bucket",
      "DeletionPolicy": "Delete"
    },
    "LoggingBucketName": {
      "Type": "AWS::SSM::Parameter",
      "Properties": {
        "Name": "/logging/bucket/name",
        "Type": "String",
        "Value": {
          "Ref": "Bucket"
        }
      }
    }
  },
  "Outputs": {
    "SampleExport": {
      "Description": "The ARN of a kms key",
      "Export": {
        "Name": "Shared-KmsKeyArn"
      },
      "Value": "FakeARN"
    }
  }
}